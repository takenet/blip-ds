{
  "name": "blip-ds-react",
  "sideEffects": false,
  "version": "0.0.0-development",
  "private": false,
  "description": "React specific wrapper for blip-ds",
  "repository": {
    "type": "git",
    "url": "https://github.com/takenet/blip-ds.git"
  },
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc -p . --outDir ./dist",
    "rollup": "rollup -c"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "@stencil/react-output-target": "^1.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "semantic-release": "^24.2.6",
    "blip-ds": "file:../",
    "np": "^10.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "release": {
    "tagFormat": "blip-ds-react@v${version}",
    "branches": [
      "master",
      "main",
      {
        "name": "v2",
        "prerelease": "rc",
        "channel": "rc"
      }
    ]
  }
}