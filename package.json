{
  "name": "blip-ds",
  "version": "0.0.0-development",
  "description": "Blip Design System",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.mjs",
  "es2017": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "unpkg": "./dist/blip-ds/blip-ds.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "generate": "stencil generate",
    "clear": "rimraf dist/ && rimraf www/ && rimraf .stencil && rimraf screenshot && rimraf loader && rimraf coverage && rimraf storybook-static && rimraf blip-ds-react/node_modules && rimraf node_modules/",
    "dev": "stencil build --docs && concurrently \"npm:build:watch\" \"npm:storybook\"",
    "start": "stencil build --dev --watch --serve --docs",
    "build": "stencil build --docs --prod",
    "build:watch": "stencil build --watch",
    "build:react": "cd ./blip-ds-react && npm install && npm run build",
    "test": "stencil test --spec --e2e --screenshot --coverage --runInBand",
    "test:e2e": "stencil test --e2e --screenshot --runInBand",
    "test:unit": "stencil test --spec --coverage",
    "test:watch": "stencil test --spec --e2e --screenshot --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "eslint": "eslint . --ext .jsx,.ts,.tsx --fix",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@stencil/core": "^4.35.1",
    "@stencil/react-output-target": "^1.1.1",
    "@stencil/sass": "^3.2.1",
    "@storybook/addon-links": "^9.0.15",
    "@storybook/react-webpack5": "^9.0.9",
    "@types/aria-query": "^5.0.4",
    "@types/color": "^4.2.0",
    "@types/conventional-commits-parser": "^5.0.1",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/jest": "^29.5.14",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/webpack": "^5.28.5",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "babel-loader": "^10.0.0",
    "babel-preset-react-app": "^10.1.0",
    "color": "^5.0.0",
    "commitizen": "^4.3.1",
    "concurrently": "^9.2.0",
    "copy-webpack-plugin": "^13.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-storybook": "^9.0.15",
    "husky": "^9.1.7",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "puppeteer": "^24.11.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.6",
    "storybook": "^9.0.15",
    "ts-node": "^10.9.2",
    "types-ramda": "^0.30.1",
    "typescript": "^5.8.3",
    "yargs": "^18.0.0"
  },
  "dependencies": {
    "@storybook/addon-docs": "^9.0.15",
    "autoprefixer": "^10.4.21",
    "blip-tokens": "^1.93.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/takenet/blip-ds.git"
  },
  "bugs": {
    "url": "https://github.com/takenet/blip-ds/issues"
  },
  "homepage": "https://github.com/takenet/blip-ds#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "MIT",
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "overrides": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@typescript-eslint/typescript-estree": "^8.18.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "typescript": "^5.8.3",
    "nth-check": "^2.1.1",
    "postcss": "^8.5.5",
    "svgo": "^3.3.2",
    "rimraf": "^6.0.1"
  },
  "exports": {
    ".": {
      "import": "./dist/blip-ds/blip-ds.esm.js",
      "require": "./dist/blip-ds/blip-ds.js"
    },
    "./dist/*": {
      "import": "./dist/*",
      "types": "./dist/*"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "types": "./loader/index.d.ts",
      "import": "./loader/index.js",
      "require": "./loader/index.cjs"
    }
  },
  "release": {
    "branches": [
      "master",
      "main",
      {
        "name": "v2",
        "prerelease": "rc",
        "channel": "rc"
      }
    ]
  }
}